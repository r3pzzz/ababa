#include <stdio.h>
#include <math.h>

int main(int argc, char *argv[])
{
    double a=0, b=0, c=100;
    double x1, x2, d=0;
    a=2;
    b=1;
    c=-1;
    
    d=pow(b,2)-4*a*c;
    
    printf("d = %lf\n", d);
    
    x1=(-b+sqrt(d))/(2*a);
    
    x2=(-b-sqrt(d))/(2*a);
    
    printf("x1=%lf\nx2=%lf", x1, x2);
    return 0;
    
}
